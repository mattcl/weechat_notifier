host: 'rabbitmq_host'
user: 'rabbitmq_user'
pass: 'secret'
vhost: '/weechat'
exchange: 'chat-notify'
notice_duration: 2.5
ignored_senders:
  - someone
xmobar:
  max_len: 80
  default_color: 'orange'
  colored_senders:
    someone-else: '#BF1111'
  ignored_senders:
    - someone

# You can specify a command to check if a message should be displayed. This is
# helpful if you use a screensaver/lockscreen that permits libnotify messages
# to appear on screen. The internal implementation will match /show/i on the
# last line of output. The default behavior is to show all messages, in the
# case where the privacy command is not specified.
# privacy_command: '/path/to/command'
